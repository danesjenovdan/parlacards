<body> <!-- removeme -->

    <%

        function getInitials(name, delimiter) {

            if(name) {

                var array = name.split(delimiter);
                var initials = '';

                for (var i = 0; i < array.length; i++) {
                    if (array[i].length > 0) {
                        initials = initials + array[i].charAt(0);
                    }
                }

                return initials;

            }

            return false;

        }
        var name = data.person.name;
        var initials = getInitials(name, ' ');
    %>

    <!-- everything above here is part of the page and not part of the card -->
    <div class="card-container card-halfling <%= className %>">
        <div class="card-header">
            <div class="card-header-border"></div>
            <h1>Stilne analize</h1> <!-- this text is the only thing you touch in .card-header -->
        </div>
        <div class="card-content">
            <div class="bgwhite poslanec">

                <div class="me_poslanec_mini clearfix">
                    <div class="col150">
                        <span class="poslanec_title">
                                        Privzdignjeno <br>besedje
                                    </span>
                    </div>
                    <div class="col100">
                        <span style="font-weight: bold;"><%= initials %></span>
                        <br /> Povprečje

                    </div>
                    <div class="col180">
                        <div class="progress smallbar avgmin">
                            <div class="progress-bar funblue" role="progressbar" aria-valuenow="<%= data.results.privzdignjeno %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= data.results.privzdignjeno %>%;">
                                <span class="sr-only"><%= data.results.privzdignjeno %></span>
                            </div>
                            <div class="progress_number" <% if (Math.round(data.results.privzdignjeno) === 0) {%> style="padding-left: 0;" <%} %>>
                                <%= Math.round(data.results.privzdignjeno); %>
                            </div>

                        </div>

                        <div class="progress smallbar avgmin">
                            <div class="progress-bar red" role="progressbar" aria-valuenow="<%= data.results.average.privzdignjeno %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= data.results.average.privzdignjeno %>%;">
                                <span class="sr-only"><%= data.results.average.privzdignjeno %></span>
                            </div>
                            <div class="progress_number" <% if (Math.round(data.results.average.privzdignjeno) === 0) {%> style="padding-left: 0;" <%} %>>
                                <%= Math.round(data.results.average.privzdignjeno); %>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="me_poslanec_mini clearfix">
                    <div class="col150">
                        <span class="poslanec_title">
                                        Preprosto <br>besedje
                                    </span>
                    </div>
                    <div class="col100">
                        <span style="font-weight: bold;"><%= initials %></span>
                        <br /> Povprečje

                    </div>
                    <div class="col180">
                        <div class="progress smallbar avgmin">
                            <div class="progress-bar funblue" role="progressbar" aria-valuenow="<%= data.results.preprosto %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= data.results.preprosto %>%;">
                                <span class="sr-only"><%= data.results.preprosto %></span>
                            </div>
                            <div class="progress_number" <% if (Math.round(data.results.preprosto) === 0) {%> style="padding-left: 0;" <%} %>>
                                <%= Math.round(data.results.preprosto); %>
                            </div>

                        </div>

                        <div class="progress smallbar avgmin">
                            <div class="progress-bar red" role="progressbar" aria-valuenow="<%= data.results.average.preprosto %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= data.results.average.preprosto %>%;">
                                <span class="sr-only"><%= data.results.average.preprosto %></span>
                            </div>
                            <div class="progress_number" <% if (Math.round(data.results.average.preprosto) === 0) {%> style="padding-left: 0;" <%} %>>
                                <%= Math.round(data.results.average.preprosto); %>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="me_poslanec_mini clearfix">
                    <div class="col150">
                        <span class="poslanec_title">
                                        Ekscesno <br>besedje
                                    </span>
                    </div>
                    <div class="col100">
                        <span style="font-weight: bold;"><%- initials %></span>
                        <br /> Povprečje

                    </div>
                    <div class="col180">
                        <div class="progress smallbar avgmin">
