<body> <!-- removeme -->


<!-- everything above here is part of the page and not part of the card -->
<!-- https://analize.parlameter.si/v1/p/getAllSpeeches/80 -->


<div class="card-container card-halfling">
    <div class="card-header">
        <div class="card-header-border"></div>
        <h1>Povezave do nje govorov</h1>
        <!-- this text is the only thing you touch in .card-header -->
    </div>
    <div class="card-content recent-activity " id="recent-speeches">
        <div class="scroller" id="scrollbar-recent-activity">
            <% var person = data.person; %>

            <% var i = 0; %>
            <% for(result in data.results){ %>


            <section>
                <span class="date"><%= data.results[result].date %></span>

                <ul>
                    <li>

                        <p><a href="<%= data.results[result].speeches[0].session_id %>"><%= data.results[result].speeches[0].session_name %></a></p>
                    </li>

                    <li>
                        <% for(var speechIndex = 0; speechIndex < data.results[result].speeches.length; speechIndex++ ){ var tmp = (speechIndex+1); %>
                            <a class="speech" href="<%= data.results[result].speeches[speechIndex].speech_id %>"><%= tmp %></a>
                        <% } %>
                    </li>
                </ul>

            </section>
            <% i++; } %>

        </div>

    </div>
    <div class="card-footer">
        <div class="card-circle-button card-share"></div>
        <div class="card-circle-button card-embed">&lt;/&gt;</div>
        <div class="card-circle-button card-info">i</div>
    </div>
</div>


<!-- everything below here is part of the page and not part of the card -->

<!-- your js file is compiled to this and later inlined (must begin with data-inline="true" )-->
