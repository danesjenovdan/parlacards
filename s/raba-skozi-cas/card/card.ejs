            <div class="card-header-border"></div>
            <h1>Raba skozi Äas</h1> <!-- this text is the only thing you touch in .card-header -->
        </div>
        <div class="card-content">

            <!-- get random id -->
            <%
                function makeid() {
                    var text = "";
                    var possible = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";

                    for( var i=0; i < 5; i++ ) {
                        text += possible.charAt(Math.floor(Math.random() * possible.length));
                    }

                    return text;
                }

                var random_id = makeid();

            %>

            <script>
                var random_id = '<%- random_id %>';
            </script>

            <div class="row poslanec">
                <div class="col-xs-12">
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs" role="tablist">
