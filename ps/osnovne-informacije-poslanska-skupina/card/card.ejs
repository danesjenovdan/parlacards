<body> <!-- removeme -->

<div class="container"> <!-- removeme -->
    <div class="row"><!-- removeme -->
        <div class="col-md-6"><!-- removeme -->


            <!-- everything above here is part of the page and not part of the card -->
            <div class="card-container card-halfling <%= className %>">
                <div class="card-header">
                    <div class="card-header-border"></div>
                    <h1>Osnovne informacije</h1>
                    <!-- this text is the only thing you touch in .card-header -->
                </div>
                <div class="card-content full">
                    <div class="card-content-front">

                        <div class="bgwhite poslanec osinfopos clearfix">

                            <div class="clearfix">

                                <% if(typeof data.headOfPG !== "undefined"){ %>
                                <div class=" clearfix">


                                    <div class="col-xs-2 parlaicon-container parlaicon-containermp text-center0 btn-lg">
                                        <span class="parlaicon parlaicon-vodja" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop">

                                        <div class="verticalmember">
                                            <div class="col-xs-3 ">
                                                <div class="image respo">

                                                    <a href="<%=urlsData.base%><%=urlsData.personLink.base%><%=urlsData.person[data.headOfPG.id].slug%><%=urlsData.personLink.pregled%>" title="<%= data.headOfPG.name %>">
                                                    <img class="media-object img-circle img-responsive"
                                                         src="https://cdn.parlameter.si/v1/parlassets/img/people/square/<%= data.headOfPG.gov_id %>.png"
                                                         alt=""/>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="col-xs-9 member_data">
                                                <h3>
                                                    <a href="<%=urlsData.base%><%=urlsData.personLink.base%><%=urlsData.person[data.headOfPG.id].slug%><%=urlsData.personLink.pregled%>" style="color: #6d6d6d;" class="funblue-light-hover"><%= data.headOfPG.name %></a>
                                                </h3>
                                                <h4>
                                                    <%
                                                        var title = "Vodja poslanske skupine";
                                                        if (data.headOfPG.gender == "f") {
                                                            title = "Vodja poslanske skupine";
                                                        } else {

                                                        } %>
                                                    <%= title %>
                                                </h4>
                                            </div>
                                        </div>
                                    </div>


                                </div>
                                <% } %>

                                <% for(item in data.viceOfPG){ %>
                                <div class=" clearfix">
                                    <div class="col-xs-2 parlaicon-container parlaicon-containermp text-center0 btn-lg">
                                        <span class="parlaicon parlaicon-namestnik" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop">
                                        <div class="verticalmember">
                                            <div class="col-xs-3 ">
                                                <div class="image respo">

                                                    <a href="<%=urlsData.base%><%=urlsData.personLink.base%><%=urlsData.person[data.viceOfPG[item].id].slug%><%=urlsData.personLink.pregled%>" title="<%= data.viceOfPG[item].name %>">
                                                    <img class="media-object img-circle img-responsive"
                                                         src="https://cdn.parlameter.si/v1/parlassets/img/people/square/<%= data.viceOfPG[item].gov_id %>.png"
                                                         alt=""/>
                                                        </a>
                                                </div>
                                            </div>
                                            <div class="col-xs-9 member_data">
                                                <h3>
                                                    <a href="<%=urlsData.base%><%=urlsData.personLink.base%><%=urlsData.person[data.viceOfPG[item].id].slug%><%=urlsData.personLink.pregled%>" style="color: #6d6d6d;" class="funblue-light-hover"><%= data.viceOfPG[item].name %></a>
                                                </h3>
                                                <h4>
                                                    <%
                                                        var title = "Namestnik";
                                                        if (data.viceOfPG[item].gender == "f") {
                                                            title = "Namestnica";
                                                        } else {

                                                        } %>
                                                    <%= title %>
                                                </h4>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <% } %>

                                <div class="clearfix ">
                                    <!--<div class="col-xs-2 parlaicon-container text-center btn-sm2">-->
                                    <div class="col-xs-2 parlaicon-container text-center0 btn-sm2">
                                        <span class="parlaicon parlaicon-skupina" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop0">
                        <span class="key">
                            Zaupalo jim je:
                            <strong><%= data.allVoters %> volivk in volivcev</strong>
                        </span>
                                    </div>
                                </div>

                                <div class="clearfix  ">
                                    <div class="col-xs-2 parlaicon-container text-center0 btn-sm2">
                                        <span class="parlaicon parlaicon-sedezi" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop0">
                        <span class="key">
                            Število poslanskih sedežev: <strong><%= data.numberOfSeats %></strong>
                        </span>
                                    </div>
                                </div>

                                <div class="clearfix  ">
                                    <div class="col-xs-2 parlaicon-container text-center0 btn-sm2">
                                        <span class="parlaicon parlaicon-kontakt" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop0">
                        <span class="key">
                            Kontakt: <strong><a href="mailto:<%= data.email %>"><%= data.social.email %></a></strong>
                        </span>
                                    </div>
                                </div>


                                <div class="clearfix  ">
                                    <div class="col-xs-2 parlaicon-container text-center0 btn-sm2">
                                        <span class="parlaicon parlaicon-omrezja" aria-hidden="true"></span>
                                    </div>
                                    <div class="col-xs-10 bordertop0">
                        <span class="key">
                            Družbena omrežja:
                            <%
                            if (data.social.facebook) {
                            %>

                            <a href="<%= data.social.facebook %>" class="socialicon-container"><div class="parlaicon parlaicon-facebook"><svg viewBox="0 0 59 100" width="59" height="100" xmlns="http://www.w3.org/2000/svg"><g><title>background</title><rect fill="none" id="canvas_background" height="402" width="582" y="-1" x="-1"/></g><g><path d="m53.515999,0.67l0,14.732l-8.761002,0q-4.799,0 -6.473,2.008999t-1.674,6.027l0,10.547001l16.350002,0l-2.176003,16.517998l-14.173996,0l0,42.355003l-17.076002,0l0,-42.355003l-14.23,0l0,-16.517998l14.23,0l0,-12.165001q0,-10.378999 5.803999,-16.099001t15.458,-5.719999q8.203003,0 12.723,0.67l-0.000999,-0.001z"/></g></svg></div></a>

                            <%
                            }
                            if (data.social.twitter) {
                            %>

                            <a href="<%= data.social.twitter %>" class="socialicon-container"><div class="parlaicon parlaicon-twitter"><svg viewBox="0 0 93 100" width="93" height="100" xmlns="http://www.w3.org/2000/svg"><g><title>background</title><rect fill="none" id="canvas_background" height="402" width="582" y="-1" x="-1"/></g><g><path id="svg_1" d="m90.402,22.768q-3.738998,5.469 -9.040001,9.319002q0.056,0.780998 0.056,2.343998q0,7.254002 -2.121002,14.480999t-6.445,13.867001t-10.295998,11.747002t-14.396999,8.146996t-18.025,3.041q-15.123001,0 -27.679002,-8.091995q1.953,0.223 4.353,0.223q12.555999,0 22.377,-7.701004q-5.858999,-0.112 -10.490999,-3.598999t-6.362,-8.901001q1.842,0.279003 3.404,0.279003q2.4,0 4.743,-0.614002q-6.25,-1.283001 -10.352,-6.222t-4.102001,-11.467999l0,-0.223q3.795,2.120998 8.147001,2.287998q-3.683,-2.454998 -5.859001,-6.417t-2.176,-8.594q0,-4.910999 2.455,-9.095999q6.752,8.314999 16.434,13.309t20.731001,5.552q-0.445999,-2.121002 -0.445999,-4.129002q0,-7.477999 5.272999,-12.750999t12.750999,-5.273q7.812004,0 13.169998,5.692q6.083,-1.172001 11.440002,-4.353001q-2.065002,6.417 -7.923996,9.933001q5.189995,-0.558001 10.378998,-2.789999l0.001999,0.000999z"/></g></svg></div></a>

                            <%
                            }
                            %>
                        </span>
                                    </div>
                                </div>

                            </div>

                        </div>
                        <!-- delete this placeholder, this is the content of your card -->
                    </div>
                    <div class="card-content-info hidden">
                        <div class="card-back-content">
                            <div class="scroller" id="tvojamtka">
                                <p class="info-text">
                                    Vsebine za to kartico smo pridobili s spletnega mesta DZ RS (poslanska skupina, starost, članstva v delovnih telesih) in s spletnega mesta DVK (število prejetih glasov). Za ostale vsebine smo se obrnili na PR službe poslanskih skupin. Podatke so nam posredovali iz NSi, SD, (takrat še) ZaAB in ZL. SMC so nas usmerili na svoje spletno mesto, SDS pa na spletno mesto državnega zbora. Manjkajoče podatke smo pridobili s pomočjo iskalnika Google in jih za morebitne popravke pred objavo ponudili vsem poslanskim skupinam.
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="card-content-embed hidden">
                        <div class="card-back-content">
                            <div class="embed-content">
                                <div class="embed-divider"></div>
                                <div class="embed-switch-container">
                                    <div class="embed-label">Podatki naj se vedno osvežujejo</div>
                                    <div class="embed-switch-big-box">
                                        <div class="embed-switch-box">
                                            <div class="embed-switch">
                                                <div class="embed-switches">
                                                    <div class="leftswitch">
                                                        DA
                                                    </div>
                                                    <div class="rightswitch">
                                                        NE
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="embed-switch-ball"></div>
                                    </div>
                                </div>
                                <div class="embed-divider"></div>
                                <div class="embed-script">
                                    <textarea class="form-control" data-id="<%= data.party.id %>" data-url="https://glej.parlameter.si/<%= cardData.group %>/<%= cardData.method %>/"><script>(function(d,script){script=d.createElement('script');script.type='text/javascript';script.async=true;script.onload=function(){iFrameResize({log:true,checkOrigin:false})};script.src = 'https://cdn.parlameter.si/v1/parlassets/js/iframeResizer.min.js';d.getElementsByTagName('head')[0].appendChild(script);}(document));</script><iframe frameborder="0" width="100%" src="https://glej.parlameter.si/<%= cardData.group %>/<%= cardData.method %>/<%= data.party.id %>/?embed=true&altHeader=true"></textarea>
                                    <button class="btn-parlameter btn-full-width btn-blue btn-copy-embed">KOPIRAJ</button>
                                </div>
                                <div class="embed-divider"></div>
                                <div class="embed-export">
                                    <div class="embed-label">Izvozi kartico kot sliko <button class="btn-parlameter btn-blue btn-margin-left">PRENESI</button></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-content-share hidden">
                        <div class="card-back-content card-content">
                            <div class="share-content">
                                <label for="share-url">Neposredna povezava do kartice</label>
                                <input type="url" class="form-control share-url" id="share-url" value="https://glej.parlameter.si/<%= cardData.group %>/<%= cardData.method %>/<%= data.party.id %>/?frame=true&altHeader=true"/>
                                <button class="btn-parlameter btn-full-width btn-blue">KOPIRAJ</button>
                            </div>
                        </div>
                    </div>
                    <!-- delete this placeholder, this is the content of your card -->
                </div>
                <div class="card-footer">
                <div class="card-logo hidden">
                     <a href="https://skoraj.parlameter.si/">
                         <img src="https://cdn.parlameter.si/v1/parlassets/img/logo-parlameter.svg" alt="parlameter logo">
                     </a>
                 </div>

                    <div class="card-circle-button card-share" data-back="share"></div>
                    <div class="card-circle-button card-embed" data-back="embed"></div>
                    <div class="card-circle-button card-info" data-back="info">i</div>
                </div>
            </div>
            <!-- everything below here is part of the page and not part of the card -->

        </div><!-- removeme -->
    </div><!-- removeme -->
</div><!-- removeme -->


